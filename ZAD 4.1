let zmiennaBoolowska = true;
console.log("Wartość zmiennej:", zmiennaBoolowska);

if (zmiennaBoolowska) {
    console.log("Zmienna ma wartość true.");
}

if (!zmiennaBoolowska) {
    console.log("Zmienna ma wartość false.");
} else {
    console.log("Zmienna nadal ma wartość true (sprawdzenie przez else).");
}

zmiennaBoolowska = !zmiennaBoolowska;

console.log("Po zmianie wartość zmiennej:", zmiennaBoolowska);

if (zmiennaBoolowska) {
    console.log("Zmienna ma teraz wartość true.");
}

if (!zmiennaBoolowska) {
    console.log("Zmienna ma teraz wartość false.");
}
